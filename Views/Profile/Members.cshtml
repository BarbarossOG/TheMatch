            <div class="col-lg-4">
                <div class="member-sidebar">
                    <div class="widget widget-filter">
                        <h4 class="widget-title">Filter</h4>
                        <div class="widget-body">
                            <div class="filter-item">
                                <h6 class="filter-title">I'm looking for:</h6>
                                <div class="filter-options">
                                    <div class="filter-option">
                                        <input type="radio" name="gender" id="male"><label for="male">Male</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="radio" name="gender" id="female"><label for="female">Female</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="radio" name="gender" id="any" checked><label for="any">Any</label>
                                    </div>
                                </div>
                            </div>
                            <div class="filter-item">
                                <h6 class="filter-title">Age Range:</h6>
                                <div class="filter-options range">
                                    <div class="range-slider"></div>
                                    <div class="range-counter">
                                        <span class="min">18</span>
                                        <span class="max">60</span>
                                    </div>
                                </div>
                            </div>
                            <div class="filter-item">
                                <h6 class="filter-title">Location:</h6>
                                <div class="filter-options location">
                                    <select class="select-bar">
                                        <option value="">Anywhere</option>
                                        <option value="">New York</option>
                                        <option value="">Los Angeles</option>
                                        <option value="">Chicago</option>
                                        <option value="">Houston</option>
                                        <option value="">Miami</option>
                                        <option value="">Seattle</option>
                                        <option value="">Boston</option>
                                        <option value="">San Francisco</option>
                                    </select>
                                </div>
                            </div>
                            <div class="filter-item">
                                <h6 class="filter-title">Interests:</h6>
                                <div class="filter-options interest">
                                    <div class="filter-option">
                                        <input type="checkbox" id="travel"><label for="travel">Travel</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="checkbox" id="music"><label for="music">Music</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="checkbox" id="food"><label for="food">Food</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="checkbox" id="sports"><label for="sports">Sports</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="checkbox" id="movies"><label for="movies">Movies</label>
                                    </div>
                                    <div class="filter-option">
                                        <input type="checkbox" id="books"><label for="books">Books</label>
                                    </div>
                                </div>
                            </div>
                            <div class="filter-item">
                                <button type="submit" class="custom-button">Apply Filter</button>
                            </div>
